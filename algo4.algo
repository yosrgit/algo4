ALGORITHM algo4
VAR
    tab : ARRAY_OF INTEGER []
    val,i,j:INTEGER
BEGIN
    FOR i FROM 1 TO 10  DO
        READ(val)
        inserted:=false
        j:=tab.length
        WHILE(j>0 AND inserted=false)
            IF (tab[j-1]>val ) THEN
                tab[j+1]=tab[j]
                j:=j-1
            ELSE
                inserted:=true
            END_IF
        END_WHILE
    tab[j]=val
    END_FOR
END